{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":assignee(codereacher15)",
    ":reviewer(codereacher15)"
  ],
 "lockFileMaintenance": {
    "enabled": false
  },
  "packageRules": [
    {
      "prHourlyLimit": 5,
      "dependencyDashboardApproval": true,
      "groupName":"update-version",
       "branchTopic":"update-version",
       "labels": [
        "renovatebot"
      ],
      "matchUpdateTypes": [
        "major"
      ]
    }
    
    
    
  ]

}

